// Miscellaneous
ItemDisplay[key]: %WHITE%Key
ItemDisplay[tsc]: %BLUE%!%WHITE%TP
ItemDisplay[isc]: %RED%!%WHITE%ID
ItemDisplay[tbk]: %BLUE%!%WHITE%TP Book
