//Find item codes at: https://pathofdiablo.com/wiki/index.php?title=Loot_Filtration_Codes
// Ignore all gold
ItemDisplay[GOLD>0]:

// Potions
ItemDisplay[rvl]: %PURPLE%[%WHITE%FULL%PURPLE%] // Full Rejuvenation Potion
ItemDisplay[hp5]: %RED%[%WHITE%HP%RED%] // Super Health Potion
ItemDisplay[mp5]: %BLUE%[%WHITE%MP%BLUE%] // Super Mana Potion
ItemDisplay[hp1 OR hp2 OR hp3 OR hp4]: // Other Health Potions
ItemDisplay[mp1 OR mp2 OR mp3 OR mp4]: // Other Mana Potions

// Gems
ItemDisplay[gcv OR gcy OR gcb OR gcg OR gcr OR gcw OR skc]: // chipped
ItemDisplay[gfv OR gfy OR gfb OR gfg OR gfr OR gfw OR skf]: // flawed
ItemDisplay[gsv OR gsy OR gsb OR gsg OR gsr OR gsw OR sku]: // standard
// Flawless
ItemDisplay[gzv]: %TAN%F Amethyst
ItemDisplay[glw]: %TAN%F Diamond
ItemDisplay[glg]: %TAN%F Emerald
