ItemDisplay[gpr]: %TAN%P Ruby
ItemDisplay[gpb]: %TAN%P Sapphire
ItemDisplay[gpy]: %TAN%P Topaz
ItemDisplay[skz]: %TAN%P Skull

// Runes
//1-16 
ItemDisplay[r01 OR r02 OR r03 OR r04 OR r05 OR r06 OR r07 OR r08 OR r09 OR r10 OR r11 OR r12 OR r13 OR r14 OR r15 OR r16]: %TAN%%RUNENAME% [%RUNENUM%]
//17-33
ItemDisplay[r17 OR r18 OR r19 OR r20 OR r21 OR r22 OR r23 OR r24 OR r25 OR r26 OR r27 OR r28 OR r29 OR r30 OR r31 OR r32 OR r33]: %ORANGE%%RUNENAME% [%RUNENUM%]

